    "truffle": "5.1.20",
    "truffle-hdwallet-provider": "1.0.17"
  },
  "devDependencies": {
    "@openzeppelin/contracts": "3.0.0-rc.0",
    "@openzeppelin/test-helpers": "0.5.5",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-prettier": "6.10.1",
