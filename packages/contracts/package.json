{
  "name": "contracts",
  "version": "0.0.1",
  "description": "> TODO: description",
  "author": "Marcelo Morgado <cmarcelom@gmail.com>",
  "homepage": "https://github.com/marcelomorgado/fyght/tree/master/packages/contracts#readme",
  "license": "MIT",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcelomorgado/fyght.git"
  },
  "scripts": {
    "clean": "npx --no-install rimraf build",
    "start": "npm run clean && npm run ganache:stop && npm run ganache:start && npm run truffle:migrate",
    "test": "npm run clean && npx --no-install truffle test",
    "ganache:start": "npx --no-install ganache-cli --mnemonic fyght --networkId 1234 > ganache.log 2>&1 &",
    "ganache:stop": "kill `ps ax | grep ganache-cli | grep -v grep | awk '{print $1}'` 2> /dev/null; exit 0",
    "truffle:migrate": "npx --no-install truffle migrate --network local",
    "lint": "npx --no-install eslint './test/{*.js,**/*.js}' && npx --no-install prettier -c './test/{*.js,**/*.js}'",
    "lint:fix": "npx --no-install eslint --fix './test/{*.js,**/*.js}' && npx --no-install prettier --write -c './test/{*.js,**/*.js}'"
  },
  "bugs": {
    "url": "https://github.com/marcelomorgado/fyght/issues"
  },
  "dependencies": {
    "@openzeppelin/contracts": "2.5.0",
    "ganache-cli": "6.9.1",
    "truffle": "5.1.13",
    "truffle-hdwallet-provider": "1.0.17"
  },
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.20.1",
    "prettier": "1.19.1",
    "rimraf": "3.0.2"
  }
}
