  },
  "scripts": {
    "clean": "npx --no-install rimraf build",
    "test": "npm run clean && npx --no-install truffle test",
    "lint": "npx --no-install eslint './test/{*.js,**/*.js}' && npx --no-install prettier -c './test/{*.js,**/*.js}'",
    "lint:fix": "npx --no-install eslint --fix './test/{*.js,**/*.js}' && npx --no-install prettier --write -c './test/{*.js,**/*.js}'",
    "deploy:extdev": "npx --no-install truffle deploy --network extdev",
    "postdeploy:extdev": "cp ./build/contracts/* ../app/src/contracts"
  },
  "bugs": {
    "url": "https://github.com/marcelomorgado/fyght/issues"
  },
